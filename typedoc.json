{
  "entryPoints": [
    "packages/core/*.ts",
    "packages/deploy-express/*.ts",
    "packages/deploy-fastify/*.ts",
    "packages/deploy-next/*.ts",
    "packages/deploy-uws/*.ts",
    "packages/deploy-next/*.ts",
    "packages/services-aws/*.ts"
  ],
  "out": ".typedocs",
  "compilerOptions": {
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noUnusedParameters": false,
  },
  "exclude": ["**/node_modules/**/*", "**/dist/**/*", "**/*.spec.ts", "**/*.d.ts"]
}
