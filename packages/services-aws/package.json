{
  "name": "@vramework/services-aws",
  "version": "1.0.0",
  "author": "yasser.fadl@gmail.com",
  "license": "UNLICENSED",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "build:esm": "tsc -b && cp package.json dist/esm/package.json",
    "build:cjs": "tsc -b tsconfig.cjs.json && cp package.json dist/cjs/package.json && sed 's/\"type\": \"module\"/\"type\": \"commonjs\"/' dist/cjs/package.json > dist/cjs/package.tmp.json && mv dist/cjs/package.tmp.json dist/cjs/package.json",
    "build": "rm -rf dist && yarn build:esm && yarn build:cjs",
    "release": "npm run build && npm test",
    "ncu": "ncu",
    "test": "echo 'No tests'"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    },
    "./*.d.ts": {
      "import": "./dist/types/*.d.ts",
      "require": "./dist/types/*.d.ts"
    }
  },
  "peerDependencies": {
    "@vramework/core": "^0.5.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.675.0",
    "@aws-sdk/client-secrets-manager": "3.675.0",
    "@aws-sdk/s3-request-presigner": "3.675.0",
    "aws-cloudfront-sign": "^3.0.2"
  },
  "devDependencies": {
    "typescript": "^5.6"
  },
  "engines": {
    "node": ">=18"
  }
}
