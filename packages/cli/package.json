{
  "name": "@pikku/cli",
  "version": "0.10.0",
  "author": "yasser.fadl@gmail.com",
  "license": "MIT",
  "bin": {
    "pikku": "dist/bin/pikku.js"
  },
  "type": "module",
  "main": "dist/bin/pikku.js",
  "module": "dist/bin/pikku.js",
  "scripts": {
    "tsc": "tsc",
    "schema": "ts-json-schema-generator -o cli.schema.json --path 'src/utils/pikku-cli-config.ts' --type 'PikkuCLIConfig'",
    "build": "yarn build:esm",
    "build:esm": "./build.sh",
    "ncu": "npx npm-check-updates -x '/.*glob.*/'",
    "release": "yarn build && npm test",
    "test": "bash run-tests.sh",
    "test:watch": "bash run-tests.sh --watch",
    "test:coverage": "bash run-tests.sh --coverage"
  },
  "dependencies": {
    "@openapi-contrib/json-schema-to-openapi-schema": "^4.2.0",
    "@pikku/core": "^0.9.12-next.0",
    "@pikku/inspector": "^0.9.6-next.0",
    "@types/cookie": "^1.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/uuid": "^11.0.0",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "commander": "^14",
    "openapi-types": "^12.1.3",
    "path-to-regexp": "^8.3.0",
    "tinyglobby": "^0.2.12",
    "ts-json-schema-generator": "^2.4.0",
    "typescript": "^5.9",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.9.1"
  },
  "engines": {
    "node": ">=18"
  }
}
