    "pino-pretty": "^13.1.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9"
  },
  "devDependencies": {
    "@types/node": "^24",
    "tsx": "^4.20.6"
  }
}
