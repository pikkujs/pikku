{
  "name": "@pikku/inspector",
  "version": "0.11.2",
  "author": "yasser.fadl@gmail.com",
  "license": "BUSL-1.1",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./schema-generator": {
      "types": "./dist/schema-generator.d.ts",
      "import": "./dist/schema-generator.js",
      "default": "./dist/schema-generator.js"
    },
    "./workflow-graph": {
      "types": "./dist/utils/workflow/graph/index.d.ts",
      "import": "./dist/utils/workflow/graph/index.js",
      "default": "./dist/utils/workflow/graph/index.js"
    }
  },
  "scripts": {
    "tsc": "tsc",
    "build": "tsc -b",
    "ncu": "npx npm-check-updates -x '/.*glob.*/'",
    "release": "yarn build && npm test",
    "test": "bash run-tests.sh",
    "test:watch": "bash run-tests.sh --watch",
    "test:coverage": "bash run-tests.sh --coverage"
  },
  "dependencies": {
    "@pikku/core": "^0.11.2",
    "path-to-regexp": "^8.3.0",
    "ts-json-schema-generator": "^2.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9",
    "zod": "^4.3.6",
    "zod-to-ts": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.12"
  },
  "engines": {
    "node": ">=18"
  }
}
