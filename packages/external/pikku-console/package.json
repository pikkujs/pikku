{
  "name": "@pikku/external-console",
  "version": "0.12.0",
  "type": "module",
  "imports": {
    "#pikku": "./.pikku/pikku-types.gen.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./.pikku/*": "./.pikku/*",
    "./.pikku/pikku-metadata.gen.json": "./.pikku/pikku-metadata.gen.json",
    "./.pikku/rpc/pikku-rpc-wirings-map.internal.gen.js": {
      "types": "./.pikku/rpc/pikku-rpc-wirings-map.internal.gen.d.ts"
    }
  },
  "files": [
    "dist",
    ".pikku"
  ],
  "scripts": {
    "prebuild": "pikku all",
    "build": "tsc && cp -r .pikku dist/",
    "pikku": "pikku all",
    "test": "tsc --noEmit"
  },
  "peerDependencies": {
    "@pikku/core": "^0.12.0"
  },
  "devDependencies": {
    "@pikku/cli": "^0.12.0",
    "@pikku/core": "^0.12.0",
    "oauth2-mock-server": "^7.1.1",
    "typescript": "^5.9"
  },
  "dependencies": {
    "@pikku/pg": "^0.12.0",
    "json-schema": "^0.4.0",
    "open": "^10.0.0",
    "postgres": "^3.4.5"
  }
}
