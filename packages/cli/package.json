{
  "name": "@pikku/cli",
  "version": "0.9.14",
  "author": "yasser.fadl@gmail.com",
  "license": "MIT",
  "bin": {
    "pikku": "dist/bin/pikku.js"
  },
  "type": "module",
  "main": "dist/bin/pikku.js",
  "module": "dist/bin/pikku.js",
  "scripts": {
    "tsc": "tsc",
    "build:esm": "tsc -b",
    "schema": "ts-json-schema-generator -o cli.schema.json --path 'src/pikku-cli-config.ts' --type 'PikkuCLIConfig'",
    "build": "yarn build:esm && yarn schema",
    "ncu": "npx npm-check-updates -x '/.*glob.*/'",
    "release": "yarn build && npm test",
    "test": "bash run-tests.sh",
    "test:watch": "bash run-tests.sh --watch",
    "test:coverage": "bash run-tests.sh --coverage"
  },
  "dependencies": {
    "@openapi-contrib/json-schema-to-openapi-schema": "^4.0.2",
    "@pikku/core": "^0.9.9",
    "@pikku/inspector": "^0.9.5",
    "@types/cookie": "^1.0.0",
    "@types/uuid": "^10.0.0",
    "chalk": "^5.5.0",
    "chokidar": "^4.0.3",
    "commander": "^14",
    "path-to-regexp": "^8.2.0",
    "tinyglobby": "^0.2.12",
    "ts-json-schema-generator": "^2.4.0",
    "typescript": "^5.9",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.1"
  },
  "engines": {
    "node": ">=18"
  }
}
