  "dependencies": {
    "@pikku/core": "^0.10.1",
    "@pikku/uws": "^0.10.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6"
  },
  "devDependencies": {
    "@types/node": "^24",
    "typescript": "^5.9"
  }
}
