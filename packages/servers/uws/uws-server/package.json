{
  "name": "@vramework/uws",
  "version": "0.5.2",
  "description": "The vramework uws server",
  "license": "MIT",
  "author": "yasser.fadl@gmail.com",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "build:esm": "tsc -b && echo '{\"type\": \"module\"}' > dist/esm/package.json",
    "build:cjs": "tsc -b tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "build": "rm -rf dist && yarn build:esm && yarn build:cjs",
    "release": "npm run build && npm test",
    "ncu": "ncu",
    "test": "echo 'No tests'"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    },
    "./*.d.ts": {
      "import": "./dist/esm/*.d.ts",
      "require": "./dist/cjs/*.d.ts"
    }
  },
  "peerDependencies": {
    "@vramework/core": "^0.5.1"
  },
  "dependencies": {
    "@types/qs": "^6",
    "@vramework/uws-handler": "^0.5.2",
    "qs": "^6.13.0",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.49.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
}
