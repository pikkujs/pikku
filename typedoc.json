{
  "entryPoints": ["./**/index.ts"],
  "entryPointStrategy": "expand",
  "excludeInternal": true,
  "out": ".typedocs",
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitAny": false,
    "resolveJsonModule": true
  },
  "exclude": [
    "**/node_modules/**/*",
    "**/dist/**/*",
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
