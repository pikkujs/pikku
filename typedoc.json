{
  "entryPoints": [
    "packages/core/src/*.ts",
    "packages/fetch/src/*.ts",
    "packages/servers/**/src/*.ts",
    "packages/cloud/**/src/*.ts",
    "packages/services/**/src/*.ts"
  ],
  "out": ".typedocs",
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitAny": false,
    "resolveJsonModule": true
  },
  "exclude": [
    "**/node_modules/**/*",
    "**/dist/**/*",
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
