{
  "name": "@pikku/templates-workflows-aws-lambda",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && esbuild ./src/handler.ts --format=esm --minify --external:\"@aws-sdk/*\" --bundle --keep-names --sourcemap --platform=node --target=node20 --outdir=dist --out-extension:.js=.mjs --metafile=meta.json",
    "ncu:pikku": "npx npm-check-updates -f '/@pikku/.*/'",
    "pikku": "pikku all",
    "test:http-fetch": "npx tsx client/http-fetch.ts",
    "test:http-sse": "npx tsx client/http-sse.ts",
    "test:rpc": "npx tsx client/rpc.ts",
    "test:websocket": "npx tsx client/websocket.ts",
    "test:queue": "npx tsx client/queue-worker.ts",
    "test:mcp": "npx tsx client/mcp.ts",
    "start": "npm run build && serverless offline",
    "deploy": "npm run build && serverless deploy",
    "deploy:dev": "npm run build && serverless deploy --stage dev",
    "deploy:prod": "npm run build && serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs:http": "serverless logs -f http -t",
    "logs:worker": "serverless logs -f queueWorker -t",
    "test:workflows": "tsx client/workflow-start.ts && tsx client/workflow-happy.ts && tsx client/workflow-unhappy.ts && tsx client/workflow-cancel.ts",
    "test": "bash run-tests.sh --workflow --url http://localhost:3000/dev",
    "postinstall": "pikku all"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.914.0",
    "@aws-sdk/client-s3": "^3.914.0",
    "@aws-sdk/client-secrets-manager": "^3.714.0",
    "@aws-sdk/client-sqs": "^3.914.0",
    "@aws-sdk/cloudfront-signer": "^3.723.0",
    "@aws-sdk/lib-dynamodb": "^3.914.0",
    "@aws-sdk/s3-request-presigner": "^3.914.0",
    "@pikku/aws-services": "^0.10.0",
    "@pikku/core": "^0.10.0",
    "@pikku/express": "^0.10.0",
    "@pikku/jose": "^0.10.0",
    "@pikku/lambda": "^0.10.0",
    "@pikku/schema-cfworker": "^0.10.0",
    "@serverless/utils": "^6.15.0",
    "eventsource": "^4.0.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@pikku/cli": "^0.10.0",
    "@pikku/fetch": "^0.10.0",
    "@pikku/websocket": "^0.10.1",
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^24",
    "@types/ws": "^8",
    "concurrently": "^9.2.1",
    "esbuild": "^0.24.0",
    "serverless": "^4.21.1",
    "serverless-dynamodb": "^0.2.56",
    "serverless-offline": "^14.4.0",
    "serverless-offline-sqs": "^8.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9"
  }
}
